<?xml version="1.0" encoding="UTF-8"?>
<module name="test-module"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:tm="urn:ietf:params:xml:ns:yang:test-module">
  <namespace uri="urn:ietf:params:xml:ns:yang:test-module"/>
  <prefix value="tm"/>
  <organization>
    <text>organization</text>
  </organization>
  <description>
    <text>example yang module</text>
  </description>
  <contact>
    <text>example@example.org</text>
  </contact>
  <container name="main">
    <leaf name="enum">
      <type name="enumeration">
        <enum name="yes">
          <value value="1"/>
        </enum>
        <enum name="no">
          <value value="2"/>
        </enum>
        <enum name="maybe">
          <value value="3"/>
        </enum>
      </type>
    </leaf>
    <leaf name="options">
      <type name="bits">
        <bit name="strict"/>
        <bit name="recursive"/>
        <bit name="logging"/>
      </type>
    </leaf>
    <leaf name="raw">
      <type name="binary"/>
    </leaf>
    <leaf name="dec64">
      <type name="decimal64">
        <fraction-digits value="2"/>
      </type>
    </leaf>
    <leaf name="i8">
      <type name="int8"/>
    </leaf>
    <leaf name="i16">
      <type name="int16"/>
    </leaf>
    <leaf name="i32">
      <type name="int32"/>
    </leaf>
    <leaf name="i64">
      <type name="int64"/>
    </leaf>
    <leaf name="ui8">
      <type name="uint8"/>
    </leaf>
    <leaf name="ui16">
      <type name="uint16"/>
    </leaf>
    <leaf name="ui32">
      <type name="uint32"/>
    </leaf>
    <leaf name="ui64">
      <type name="uint64"/>
    </leaf>
    <leaf name="empty">
      <type name="empty"/>
    </leaf>
    <leaf name="boolean">
      <type name="boolean"/>
    </leaf>
    <leaf name="string">
      <type name="string"/>
    </leaf>
    <leaf name="id_ref">
      <type name="identityref">
        <base name="base_id"/>
      </type>
    </leaf>
    <leaf-list name="numbers">
      <type name="uint8"/>
    </leaf-list>
  </container>
  <identity name="base_id"/>
  <identity name="id_1">
    <base name="base_id"/>
  </identity>
  <identity name="id_2">
    <base name="base_id"/>
  </identity>
  <list name="list">
    <key value="key"/>
    <leaf name="key">
      <type name="string"/>
    </leaf>
    <leaf name="id_ref">
      <type name="identityref">
        <base name="base_id"/>
      </type>
    </leaf>
    <leaf name="union">
      <type name="union">
        <type name="uint8"/>
        <type name="enumeration">
          <enum name="infinity"/>
        </type>
      </type>
    </leaf>
    <container name="wireless">
      <presence value="wireless is enabled"/>
      <leaf name="vendor_name">
        <type name="string"/>
      </leaf>
    </container>
  </list>
  <container name="transfer">
    <choice name="how">
      <default value="interval"/>
      <case name="interval">
        <leaf name="interval">
          <type name="uint16"/>
          <default value="30"/>
          <units name="minutes"/>
        </leaf>
      </case>
      <case name="daily">
        <leaf name="daily">
          <type name="empty"/>
        </leaf>
        <leaf name="time-of-day">
          <type name="string"/>
          <units name="24-hour-clock"/>
          <default value="1am"/>
        </leaf>
      </case>
      <case name="manual">
        <leaf name="manual">
          <type name="empty"/>
        </leaf>
      </case>
    </choice>
  </container>
  <container name="location">
    <leaf name="name">
      <type name="string"/>
    </leaf>
    <leaf name="longitude">
      <type name="string"/>
      <mandatory value="true"/>
    </leaf>
    <leaf name="latitude">
      <type name="string"/>
      <mandatory value="true"/>
    </leaf>
  </container>
  <leaf name="hexnumber">
    <type name="string">
      <length value="0..4"/>
      <pattern value="[0-9a-fA-F]*"/>
    </type>
  </leaf>
  <container name="interface">
    <leaf name="ifType">
      <type name="enumeration">
        <enum name="ethernet"/>
        <enum name="atm"/>
      </type>
    </leaf>
    <leaf name="ifMTU">
      <type name="uint32"/>
    </leaf>
    <must condition="ifType != 'ethernet' or (ifType = 'ethernet' and ifMTU = 1500)">
      <error-message>
        <value>An ethernet MTU must be 1500</value>
      </error-message>
    </must>
    <must condition="ifType != 'atm' or (ifType = 'atm' and ifMTU &lt;= 17966 and ifMTU &gt;= 64)">
      <error-message>
        <value>An atm MTU must be  64 .. 17966</value>
      </error-message>
    </must>
  </container>
  <list name="user">
    <description>
      <text>This is a list of users in the system.</text>
    </description>
    <ordered-by value="user"/>
    <config value="true"/>
    <key value="name"/>
    <leaf name="name">
      <type name="string"/>
    </leaf>
    <leaf name="type">
      <type name="string"/>
    </leaf>
    <leaf name="full-name">
      <type name="string"/>
    </leaf>
  </list>
  <leaf-list name="ordered-numbers">
    <ordered-by value="user"/>
    <type name="uint8"/>
  </leaf-list>
</module>
