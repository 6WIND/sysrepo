module augm_leafref_m1 {
    namespace "test:augmleafref";
    prefix alr;

    import small-module {
        prefix sm;
    }

    container augleafrefcontainer {
        leaf name {
            type string;
        }
    }

    typedef alr-ref {
        type leafref {
            path "/alr:augleafrefcontainer/alr:name";
        }
    }

    augment "/sm:item" {
        leaf augleaf {
            type alr-ref;
        }
    }
}